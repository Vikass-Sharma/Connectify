{
    "name": "tinder-clone",
    "version": "1.0.0",
    "description": "A full-stack Tinder clone built with React, Node.js, Socket.io, and MongoDB",
    "main": "api/server.js",
    "scripts": {
        "dev": "nodemon api/server.js",
        "build": "npm install && npm install --prefix client && npm run build --prefix client",
        "start": "node api/server.js",
